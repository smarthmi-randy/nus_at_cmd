/ {
    aliases {
        atcmduart = &uart30;
        uart20 = &uart20;
        uart21 = &uart21;
    };
};

&uart30 {
    status = "okay";
    current-speed = <115200>;
};

&uart21 {
    status = "okay";
    pinctrl-0 = <&uart21_default>;
    pinctrl-names = "default";
    current-speed = <115200>;
};

&pinctrl {
    uart21_default: uart21_default {
        group1 {
            psels = <NRF_PSEL(UART_TX, 1, 0)>, <NRF_PSEL(UART_RX, 1, 1)>;
        };
    };
};
